	<HediffDef>
		<defName>AT_glue</defName>
		<hediffClass>HediffWithComps</hediffClass>
		<label>Combat glue</label>
		<labelNoun>Combat glue</labelNoun>
		<description>Combat glue is on it.</description>
		<defaultLabelColor>(0.2,1,0.5)</defaultLabelColor>
		<scenarioCanAdd>false</scenarioCanAdd>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-3.5</severityPerDay>
				<showHoursToRecover>true</showHoursToRecover>
			</li>
			<li Class="HediffCompProperties_Effecter">
				<stateEffecter>Combatglue</stateEffecter>
				<severityIndices>0~5</severityIndices>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>0</minSeverity>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<statFactors>
					<MoveSpeed>0.9</MoveSpeed>
				</statFactors>
			</li>
			<li>
				<minSeverity>0.3</minSeverity>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.1</offset>
					</li>
				</capMods>
				<statFactors>
					<MoveSpeed>0.8</MoveSpeed>
				</statFactors>
			</li>
			<li>
				<minSeverity>0.45</minSeverity>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.15</offset>
					</li>
				</capMods>
				<statFactors>
					<MoveSpeed>0.7</MoveSpeed>
				</statFactors>
			</li>
			<li>
				<minSeverity>0.6</minSeverity>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
				<statFactors>
					<MoveSpeed>0.6</MoveSpeed>
				</statFactors>
			</li>
		</stages>
	</HediffDef>


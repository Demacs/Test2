	<ThingDef ParentName="Anty_HeadgearBase"><!--안경-->
		<defName>AT_AP_Glass_A</defName>
		<label>Anty Glass</label>
		<description>These are glasses worn by Anties. It is designed to fit compound eyes, so the average person cannot wear it.</description>

		<recipeMaker>
			<researchPrerequisite>AntyClothing</researchPrerequisite>
		</recipeMaker>

		<graphicData>
			<texPath>Antylike/Apparel/AT_Glass</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>

		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>5</costStuffCount>

		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToMake>300</WorkToMake>
			<Mass>0.02</Mass>
			<Flammability>0</Flammability>

			<StuffEffectMultiplierArmor>0.0</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.0</StuffEffectMultiplierInsulation_Heat>

			<ArmorRating_Sharp>0</ArmorRating_Sharp>
			<ArmorRating_Blunt>0</ArmorRating_Blunt>
			<ArmorRating_Heat>0.1</ArmorRating_Heat>
			<Insulation_Cold>0</Insulation_Cold>
			<Insulation_Heat>0</Insulation_Heat>
			<EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<SocialImpact>0.2</SocialImpact>
		</equippedStatOffsets>
		
		<generateAllowChance>0.12</generateAllowChance>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<careIfDamaged>false</careIfDamaged>
			<wearPerDay>0</wearPerDay>
			<bodyPartGroups>
				<li>Eyes</li>
			</bodyPartGroups>
			<!-- <wornGraphicPath>Antylike/Apparel/AT_Glass</wornGraphicPath> -->
			<wornGraphicPaths>
				<li>Antylike/Apparel/AT_GlassA</li>
				<li>Antylike/Apparel/AT_GlassB</li>
			</wornGraphicPaths>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Overhead</li>
			</layers>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
			<tags>
				<li>AntyALL</li>
				<li>AntyShooter</li>
				<li>AntyShooterHEAD</li>
			</tags>
		</apparel>
	</ThingDef>


	<ThingDef ParentName="BaseWeaponTurret">
		<defName>Gun_RecycleTurretD</defName>
		<label>Recycle turret gun</label>
		<description>A simple automatic gun made to be mounted on a turret.</description>
		<graphicData>
			<texPath>Things/Building/Turret/Red_Turret_Gun</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<AccuracyTouch>0.90</AccuracyTouch>
			<AccuracyShort>0.85</AccuracyShort>
			<AccuracyMedium>0.30</AccuracyMedium>
			<AccuracyLong>0.1</AccuracyLong>
			<RangedWeapon_Cooldown>180</RangedWeapon_Cooldown>
			<DeteriorationRate>0</DeteriorationRate>
			<Mass>5</Mass>
			<Flammability>0</Flammability>
			<AT_Acid_damage>0.3</AT_Acid_damage>
			<AT_Acid_damage_duration>40</AT_Acid_damage_duration>
			<AT_Acid_damage_destruction>35</AT_Acid_damage_destruction>
		</statBases>
		<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<defaultProjectile>Bullet_Shell_AT_Red_Turret</defaultProjectile>
				<warmupTime>3</warmupTime>
				<minRange>12.9</minRange>
				<range>500</range>
				<burstShotCount>1</burstShotCount>
				<ticksBetweenBurstShots>80</ticksBetweenBurstShots>
				<soundCast>ShotAnty_A</soundCast>
				<soundCastTail>GunTail_Light</soundCastTail>
				<soundAiming>AimingAnty_A</soundAiming>
				<muzzleFlashScale>8</muzzleFlashScale>
				<stopBurstWithoutLos>false</stopBurstWithoutLos>
				<consumeFuelPerShot>1</consumeFuelPerShot>
				<requireLineOfSight>false</requireLineOfSight>
			</li>
		</verbs>
	</ThingDef>
	

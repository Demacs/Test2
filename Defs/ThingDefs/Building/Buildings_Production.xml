	<ThingDef ParentName="BuildingBase">
		<defName>AntyExoskeleton_Production</defName>
		<label>Exoskeleton Auto Mine</label>
		<description>A device made to harvest exoskeleton without the inefficient sacrifice of Anties.  The built-in AI core regularly creates an exoskeleton based on the Anty's DNA injected into the device.\nAphid extract can be used to increase production.</description>
		<descriptionHyperlinks>
			<ThingDef>AntyExoskeleton</ThingDef>
			<ThingDef>Anty_Recyclecore</ThingDef>
			<ThingDef>AT_Aphidextract</ThingDef>
		</descriptionHyperlinks>
		<graphicData>
			<texPath>Things/Building/Production/Copy_A</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2.5,2.5)</drawSize>
			<shadowData>
				<volume>(1,1,0.5)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<pathCost>100</pathCost>
		<blockWind>true</blockWind>
		<fillPercent>1</fillPercent>
		<rotatable>false</rotatable>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>4500</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>-25</Beauty>
			<Mass>30</Mass>
		</statBases>
		<tickerType>Normal</tickerType>
		<canOverlapZones>false</canOverlapZones>
		<size>(2,1)</size>
		<costList>
			<Steel>250</Steel>
			<ComponentIndustrial>3</ComponentIndustrial>
			<Anty_Recyclecore>1</Anty_Recyclecore>
		</costList>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>300</basePowerConsumption>
				<!-- <powerUpgrades>
					<li>
						<researchProject>AT_PowerEfficiency_A</researchProject>
						<factor>0.5</factor>
					</li>
				</powerUpgrades> -->
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Spawner">
				<requiresPower>true</requiresPower>
				<thingToSpawn>AntyExoskeleton</thingToSpawn>
				<spawnCount>40</spawnCount>
				<spawnIntervalRange><min>181335</min><max>321869</max></spawnIntervalRange> <!--5-7-->
				<writeTimeLeftToSpawn>true</writeTimeLeftToSpawn>
				<showMessageIfOwned>false</showMessageIfOwned>
			</li>
			<li Class="SpawnerAnty.CompProperties_SpawnerAnty">
				<requiresFuel>true</requiresFuel>
				<thingToSpawn>AntyExoskeleton</thingToSpawn>
				<spawnCount>55</spawnCount>
				<spawnIntervalRange><min>181335</min><max>321869</max></spawnIntervalRange> <!--5-7-->
				<writeTimeLeftToSpawn>true</writeTimeLeftToSpawn>
				<showMessageIfOwned>false</showMessageIfOwned>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>5.0</fuelConsumptionRate>
				<fuelCapacity>20.0</fuelCapacity>
				<fuelFilter>
				  <thingDefs>
					<li>AT_Aphidextract</li>
				  </thingDefs>
				</fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<consumeFuelOnlyWhenPowered>false</consumeFuelOnlyWhenPowered>
				<initialAllowAutoRefuel>false</initialAllowAutoRefuel>
				<drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
			</li>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>4</explosiveRadius>
				<explosiveDamageType>AT_AcidBomb</explosiveDamageType>
			</li>
		</comps>

		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Antybuilding</designationCategory>
		<designationHotKey>Misc1</designationHotKey>
		<building>
			<allowAutoroof>false</allowAutoroof>
		</building>
		<constructEffect>ConstructMetal</constructEffect>
		<researchPrerequisites>
			<li>Mediumcolony</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
	</ThingDef>
	

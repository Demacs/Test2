	<HediffDef>
		<defName>AntyDreadnoughtbirth</defName>
		<label>Anty Dreadnought</label>
		<description>Anty Dreadnought.</description>
		<isBad>false</isBad>
		<hediffClass>HediffWithComps</hediffClass>
		<scenarioCanAdd>false</scenarioCanAdd>
		<initialSeverity>0.01</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<everCurableByItem>false</everCurableByItem>
		<comps>
			<li Class="HediffCompProperties_Discoverable">
				<sendLetterWhenDiscovered>true</sendLetterWhenDiscovered>
				<discoverLetterLabel>Dreadnought birth</discoverLetterLabel>
				<discoverLetterText>{0}</discoverLetterText>
				<letterType>AT_Soldierbirth</letterType>
				<messageType>PositiveEvent</messageType>
			</li>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>100</severityPerDay>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>0</minSeverity>
				<becomeVisible>false</becomeVisible>
			</li>
			<li>
				<minSeverity>0.02</minSeverity>
				<painFactor>0.1</painFactor>
				<statFactors>
					<IncomingDamageFactor>0.7</IncomingDamageFactor>
					<InjuryHealingFactor>1.25</InjuryHealingFactor>
				</statFactors>
				<makeImmuneTo>
					<li>GutWorms</li>
					<li>MuscleParasites</li>
					<li>FibrousMechanites</li>
					<li>SensoryMechanites</li>
					<li>WoundInfection</li>
					<li>Flu</li>
				</makeImmuneTo>
			</li>
		</stages>
	</HediffDef>
	

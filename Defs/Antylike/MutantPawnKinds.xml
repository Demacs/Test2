	<PawnKindDef ParentName="AntyBasePawnKind">
		<defName>Anty_Commander_Mutant</defName>
		<label>Anty Commander</label>
		<combatPower>130</combatPower>
		<isFighter>true</isFighter>
		<trader>true</trader>
		<factionLeader>true</factionLeader>
		<forceNormalGearQuality>false</forceNormalGearQuality>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<maxGenerationAge>25</maxGenerationAge>
		 
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>ATlarva_B</li>
					<li>ATAmigo_Commander</li>
				</categories>
			</li>
		</backstoryFiltersOverride>				
		<apparelTags>
			<li>AntyALL</li>
			<li>BeltDefensePop</li>
			<li>AntyCommender</li>
		</apparelTags>
		<apparelMoney>
			<min>800</min>
			<max>1100</max>
		</apparelMoney>
		<apparelRequired>
			<li>AT_AP_Additionalarmorplate_A</li>
			<li>AT_AP_Shooter_A</li>
			<li>AT_AP_Head_B</li>
			<li>AT_AP_CO_B</li>
			<li MayRequire="Ludeon.RimWorld.Royalty">Apparel_PackBroadshield</li>
		</apparelRequired>
		<weaponTags>
			<li>AcidRifle_R</li>
			<li>AT_Proty_spawner</li>
		</weaponTags>
		<weaponMoney>
			<min>1400</min>
			<max>2500</max>
		</weaponMoney>
		<techHediffsChance>1</techHediffsChance>
		<techHediffsMoney>
			<min>800</min>
			<max>1400</max>
		</techHediffsMoney>
		<techHediffsTags>
			<li>Advanced</li>
		</techHediffsTags>
		<techHediffsRequired>
			<li>AT_Reinforcedexoskeleton_Thing</li>
			<li>AT_Reinforcedexoskeleton_Thing</li>
		</techHediffsRequired>
		<apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
		<biocodeWeaponChance>1</biocodeWeaponChance>
		<startingHediffs>
			<li>
				<def>AntySoldier</def>
				<chance>1</chance>
			</li>
		</startingHediffs>
	</PawnKindDef>
	

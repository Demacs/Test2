				<defaultProjectile>Bullet_Fake_Laser</defaultProjectile>
				<spawnDef>Proj_AT_Redburst</spawnDef>
				<rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
			</li>
		</verbs>
	</ThingDef>

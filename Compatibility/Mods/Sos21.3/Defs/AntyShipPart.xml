	<ThingDef ParentName="BuildingBase">
		<defName>AT_Ship_ComputerCore</defName>
		<label>Anty ship core</label>
		<designatorDropdown>Ship_Bridges</designatorDropdown>
		<thingClass>Building_ShipBridge</thingClass>
		<graphicData>
			<texPath>Things/Building/Ship/ShipComputerCore_Anty</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,3)</drawSize>
		</graphicData>

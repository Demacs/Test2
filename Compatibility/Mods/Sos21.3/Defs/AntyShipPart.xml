		<altitudeLayer>WorldClipper</altitudeLayer>
	</ThingDef>
	<ThingDef ParentName="BaseShipArtilleryWeapon">
		<defName>ShipTurret_AT_RedburstTOP</defName>
		<label>Red-Burst</label>
		<description>Red-Burst cannon. Short-ranged but devastating.</description>
		<graphicData>
			<texPath>Things/Building/Ship/ShipTurret_AT_RedburstTOP</texPath>
		</graphicData>
		<verbs>
			<li>
				<label>shoot Red-Burst</label>
				<verbClass>Verb_LaunchProjectileShip</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<range>12.9</range>
				<warmupTime>0</warmupTime>
				<noiseRadius>4</noiseRadius>
				<burstShotCount>10</burstShotCount>
				<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
				<requireLineOfSight>false</requireLineOfSight>
				<targetParams>
					<canTargetLocations>true</canTargetLocations>

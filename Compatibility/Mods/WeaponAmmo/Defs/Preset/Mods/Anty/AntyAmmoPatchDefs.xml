	<Reload.Defs.AmmoDef ParentName="ResourceBase">
		<defName>AT_Ammo_Acid_Special</defName>
		<label>Anty Acid capsule</label>
		<description>Anty Acid capsule.</description>
		<stackLimit>250</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<thingCategories>
			<li>R_Category_Ammo</li>
		</thingCategories>
		<tradeTags>
			<li>Ammo_Anty</li>
		</tradeTags>
		<graphicData>
			<texPath>Things/Weapons/AT_Ammo_A</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<intricate>true</intricate>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<Mass>0.05</Mass>
			<Flammability>0.2</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
			<MarketValue>4.8</MarketValue>
		</statBases>
		<costList>
			<AntyExoskeleton>2</AntyExoskeleton>
		</costList>
		<ammoType>Special</ammoType>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>2</explosiveRadius>
				<explosiveDamageType>AT_AcidBomb</explosiveDamageType>
				<explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.7</startWickHitPointsPercent>
				<wickTicks>30~60</wickTicks>
			</li>
		</comps>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Smith</soundWorking>
			<productCount>10</productCount>
			<bulkRecipeCount>30</bulkRecipeCount>
			<recipeUsers>
				<li>AntyCopyTableMachining</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>3</Crafting>
			</skillRequirements>
			<researchPrerequisite>AntyAcidweapon_B</researchPrerequisite>
			<unfinishedThingDef>R_Industrial_Ammo_Unfinish</unfinishedThingDef>
		</recipeMaker>
	</Reload.Defs.AmmoDef>
	

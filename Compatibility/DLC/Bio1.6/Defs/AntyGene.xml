<Defs>
	
	<ThingDef ParentName="ResourceBase">
		<defName>Anty_Generestorationcapsule</defName>
		<label>Gene restoration capsule</label>
		<description>It is a gene restoration agent made by mixing gene packs.</description>
		<descriptionHyperlinks>
			<ThingDef>Genepack</ThingDef>
			<HediffDef>XenogermReplicating</HediffDef>
			<RecipeDef>Anty_Generestoration</RecipeDef>
		</descriptionHyperlinks>
		<graphicData>
			<texPath>Things/Item/Anty_Generestorationcapsule</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>3</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<useHitPoints>false</useHitPoints>
		<statBases>
			<MarketValue>400</MarketValue>
			<Mass>2</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<intricate>true</intricate>
		<thingCategories>
			<li>ManufacturedAntyThingCategoryDef</li>
		</thingCategories>
		<recipeMaker>
			<researchPrerequisites>
				<li>Anty_Gene_Biotech</li>
				<li>GeneProcessor</li>
			</researchPrerequisites>
				<recipeUsers>
					<li>AntyCopyTableMachining</li>
				</recipeUsers>
		</recipeMaker>
		<costList>
			<Genepack>3</Genepack>
		</costList>
	</ThingDef>
	
	<RecipeDef ParentName="SurgeryFlesh">
		<defName>Anty_Generestoration</defName>
		<label>Gene restoration</label>
		<description>Remove genes regrowing.</description>
		<workerClass>Recipe_RemoveHediff</workerClass>
		<jobString>Removing genes regrowing.</jobString>
		<workAmount>2000</workAmount>
		<isViolation>false</isViolation>
		<targetsBodyPart>false</targetsBodyPart>
		<removesHediff>XenogermReplicating</removesHediff>
		<successfullyRemovedHediffMessage>{0} has successfully restoration {1}'s gene.</successfullyRemovedHediffMessage>
		<skillRequirements>
		<Medicine>10</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Anty_Generestorationcapsule</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>Anty_Generestorationcapsule</li>
			</thingDefs>
		</fixedIngredientFilter>
		<uiIconThing>Anty_Generestorationcapsule</uiIconThing>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
	</RecipeDef>

	<ThingDef ParentName="ProtyBase">
		<defName>Anty_swarmy</defName>
		<label>Swarmy</label>
		<description>It is a mutant proty created by Transition Refleum.\nIn exchange for losing the ability to reproduce, it has gained the ability to range attack.</description>
		<race>
			<maxPreyBodySize>3.1</maxPreyBodySize>
		</race>
		<statBases>
			<MarketValue>500</MarketValue>
			<ToxicResistance>-0.25</ToxicResistance>
			<ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">-0.25</ToxicEnvironmentResistance>
			<MoveSpeed>4.2</MoveSpeed>
			<ArmorRating_Blunt>0.14</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.45</ArmorRating_Sharp>
			<Fertility MayRequire="Ludeon.RimWorld.Biotech">0</Fertility>
			
			<VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
			

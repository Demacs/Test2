	<GeneDef>
		<defName>AntyAcidResistant</defName>
		<label>Acid proof</label>
		<description>Carriers of this gene have special fast-acting sweat glands and acid-resistant skin. They only take low of the damage from normal acid.</description>
		<iconPath>Icon/AntyGene/AntyAcidResistant</iconPath>
		<displayCategory>Anty_Gene</displayCategory>
		<displayOrderInCategory>40</displayOrderInCategory>
		<damageFactors>
		  <AcidBurn>0.25</AcidBurn>
		  <AT_AcidDamage>0.5</AT_AcidDamage>
		  
		  <AT_AcidBullet>0.8</AT_AcidBullet>
		  <AT_AcidBomb>0.8</AT_AcidBomb>
		</damageFactors>
		<biostatMet>-1</biostatMet>
		<descriptionHyperlinks>
			<HediffDef>Chemicaldissolution</HediffDef>
		</descriptionHyperlinks>
	</GeneDef>
	
	<GeneDef>
		<defName>SuperAntyAcidResistant</defName>

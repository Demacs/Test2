	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>RimJobWorld</li>
		</mods>
		<match Class="PatchOperationReplace">
			<xpath>/Defs/AlienRace.ThingDef_AlienRace[@Name = "AT_PawnBase"]/alienRace/generalSettings/forcedRaceTraitEntries</xpath>
			<value>
				<forcedRaceTraitEntries>
					<li>
						<defName>Bisexual</defName>
						<chance>25</chance>
					</li>
					<li>
						<defName>AT_Solidexoskeleton</defName>
						<chance>8</chance>
					</li>
				</forcedRaceTraitEntries>
			</value>
		</match>

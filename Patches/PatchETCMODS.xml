    </mods>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Anty" or defName="AT_Hiveguard" or defName="AT_dreadnought"]/alienRace/thoughtSettings/cannotReceiveThoughts</xpath>
